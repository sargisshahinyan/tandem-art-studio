<!DOCTYPE html>
<html lang="en">
    <% include head.ejs %>
    <body>
        <main class="wrapper">
            <% include leftBar.ejs %>
            <article class="container-fluid pl-5 pr-5 pb-5">
                <% if (activePage === '/home') { %>
                    <% include home.ejs %>
                <% } %>
                <% if (activePage === '/about') { %>
                    <% include about.ejs %>
                <% } %>
                <% if (activePage === '/team') { %>
                    <% include team.ejs %>
                <% } %>
                <% if (activePage === '/services') { %>
                    <% include services.ejs %>
                <% } %>
                <% if (activePage === '/clients') { %>
                    <% include clients.ejs %>
                <% } %>
                <% if (activePage === '/portfolio') { %>
                    <% include portfolio.ejs %>
                <% } %>
                <% if (/^\/portfolio\/\d+$/.test(activePage)) { %>
                    <% include portfolioData.ejs %>
                <% } %>
            </article>
        </main>
    </body>
</html>
