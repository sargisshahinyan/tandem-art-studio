<aside class="menu">
    <a href="/" target="_blank" class="logo">
        <img class="img-fluid" src="/img/logo.svg" alt="Logo">
    </a>
    <nav>
        <ul>
            <% NAV_TABS.forEach(tab => { %>
                <li class="tab <%= tab.path === activePath ? 'active' : '' %>">
                    <a href="<%= tab.path %>"><%= tab.title %></a>
                </li>
            <% }) %>
        </ul>
    </nav>
    <a href="<%= baseUrl %>/logout" class="btn btn-danger log-out">Sign out</a>
</aside>
